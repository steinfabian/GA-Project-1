<%= form_for @customer do |f| %>
<ul>
<li><%= f.label :first_name, 'First name' %>
	<%= f.text_field :first_name %>
<li><%= f.label :last_name, 'Last name' %>
	<%= f.text_field :last_name %>
<li><%= f.label :unit_number, 'Unit number' %>
	<%= f.text_field :unit_number %>
<li><%= f.label :street_number, 'Street number' %>
	<%= f.text_field :street_number %>
<li><%= f.label :street_name, 'Street name' %>
	<%= f.text_field :street_name %>
<li><%= f.label :suburb, 'Suburb' %>
	<%= f.text_field :suburb %>
<li><%= f.label :postcode, 'Postcode' %>
	<%= f.text_field :postcode %>
<li><%= f.label :email, 'Email' %>
	<%= f.text_field :email %>
<li><%= f.label :password, 'Password' %>
	<%= f.password_field :password%>
<li><%= f.label :password_confirmation, 'Password confirmation' %>
	<%= f.password_field :password_confirmation %>
</ul>
<button>Save</button>
<% end %>